<Configuration>
  <Display>
    <Size width="1920" height="1080" />
    <Scale>2</Scale>
    <UseVerticalSync>true</UseVerticalSync>
    <IsFullscreen>false</IsFullscreen>
    <IsFullscreenDesktop>false</IsFullscreenDesktop>
  </Display>
  <Audio>
    <Frequency>44100</Frequency>
    <SoundChannels>1</SoundChannels>
    <MixingChannels>64</MixingChannels>
    <ChunkSize>2048</ChunkSize>
    <SoundVolume>30</SoundVolume>
    <MusicVolume>7</MusicVolume>
    <SoundOn>true</SoundOn>
    <MusicOn>true</MusicOn>
    <MusicRpcServerPath>MidiProc.exe</MusicRpcServerPath>
  </Audio>
  <Font>
    <Fontt>clacon.ttf</Fontt>
    <ConsoleFont font="clacon.ttf" size="20" />
  </Font>
  <Assets>
    <AssetsFolder>/switch/claw/</AssetsFolder>
    <RezArchive>CLAW.REZ</RezArchive>
    <CustomArchive>ASSETS.ZIP</CustomArchive>
    <ResourceCacheSize>100</ResourceCacheSize>
    <TempDir>/switch/claw/</TempDir>
    <SavesFile>SAVES.XML</SavesFile>
  </Assets>
  <Console>
    <BackgroundImagePath>console02.tga</BackgroundImagePath>
    <StretchBackgroundImage>
    </StretchBackgroundImage>
    <WidthRatio>1.0</WidthRatio>
    <HeightRatio>0.5</HeightRatio>
    <LineSeparatorHeight>3</LineSeparatorHeight>
    <CommandPromptOffsetY>10</CommandPromptOffsetY>
    <ConsoleAnimationSpeed>0.7</ConsoleAnimationSpeed>
    <FontPath>clacon.ttf</FontPath>
    <FontColor r="255" g="255" b="255" />
    <FontHeight>20</FontHeight>
    <LeftOffset>5</LeftOffset>
    <CommandPrompt>&gt; </CommandPrompt>
  </Console>
  <GlobalOptions>
    <MaxJumpSpeed>8.8</MaxJumpSpeed>
    <MaxFallSpeed>14.0</MaxFallSpeed>
    <IdleSoundQuoteInterval>15000</IdleSoundQuoteInterval>
    <PlatformSpeedModifier>0.015</PlatformSpeedModifier>
    <MaxJumpHeight>155</MaxJumpHeight>
    <PowerupMaxJumpHeight>205</PowerupMaxJumpHeight>
    <StartLookUpOrDownTime>1500</StartLookUpOrDownTime>
    <MaxLookUpOrDownDistance>250</MaxLookUpOrDownDistance>
    <LookUpOrDownSpeed>250</LookUpOrDownSpeed>
    <ClawRunningSpeed>4.5</ClawRunningSpeed>
    <SpringBoardSpringHeight>UNUSED</SpringBoardSpringHeight>
    <SpringBoardSpringSpeed>11</SpringBoardSpringSpeed>
    <UseAlternateControls>false</UseAlternateControls>
    <ClawMinFallHeight>500</ClawMinFallHeight>
    <LoadAllLevelSaves>false</LoadAllLevelSaves>
    <ShowFps>false</ShowFps>
    <ShowPosition>false</ShowPosition>
  </GlobalOptions>
  <DebugOptions>
    <SkipMenu>false</SkipMenu>
    <SkipMenuToLevel>11</SkipMenuToLevel>
    <LastImplementedLevel>11</LastImplementedLevel>
    <SkipBossFightIntro>false</SkipBossFightIntro>
    <CpuDelay>0</CpuDelay>
  </DebugOptions>
</Configuration>
